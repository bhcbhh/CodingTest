#include <stdio.h>
#include <stdbool.h>
#include <stdlib.h>


using namespaces std;
// A_len은 배열 A의 길이입니다.
// B_len은 배열 B의 길이입니다.
int solution(int A[], size_t A_len, int B[], size_t B_len) {
   
   
    
    for(int j = 0; j < A_len-1; j++){
		for(int i = 0; i <A_len-1; i++){
        if(A[i] > A[i+1]){
            int temp = A[i];
            A[i] = A[i+1];
            A[i+1] = temp;
        }  
       }
    }
    
    for(int j = 0; j < B_len-1; j++){
		for(int i = 0; i < B_len-1; i++){
        if(B[i] < B[i+1]){
            int temp = B[i];
            B[i] = B[i+1];
            B[i+1] = temp;
        }  
       }
    }
    
    int answer = 0;
    
    for(int i = 0; i < A_len; i++){
       answer+= A[i] * B[i];    
    }
    
    return answer;
    
    
    
}
